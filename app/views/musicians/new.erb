<%= form_for @musician do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :instrument %>
  <%= f.text_field :instrument %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :time_for_music %>
  <%= f.number_field :time_for_music %>
  <%= f.submit "Submit" %>

<% end %>
